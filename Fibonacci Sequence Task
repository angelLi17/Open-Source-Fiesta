import java.util.*;
import java.io.*;

public class Tasks {
  public static int[] FibonacciSequence(int x) {
    int[] answer = new int[x];
    for (int i = 0; i < x; i++) {
      if (i == 0) answer[0] = 0;
      else if(i == 1) answer[1] = 1;
      else {
        answer[i] = (answer[i-1] + answer[i-2]);
      }
    }
    return answer;
  }
}
